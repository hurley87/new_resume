<!DOCTYPE html>
<html>
<head>
  <title>David Hurley</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
		<header>
			<h1 class="center">David B Hurley</h1>
			<h2 class="center">Web Developer</h2>
			<% if current_user %>
				<%= link_to "Quit", sessions_destroy_path %>
				<%= link_to "Play Again", edit_user_path(current_user) %>
			<% end %>
		</header>
		<div class="container">
			<%= yield %>
		</div>
</body>
</html>
