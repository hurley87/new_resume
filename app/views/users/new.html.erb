	<line id="arm2" x1="150" y1="125" x2="105" y2="100" stroke="#000" stroke-width="4"/>
</svg>
<br>
<br>
<br>
<br>
<div class="letters">
	<div class="row center"></div>	

	<div class="row center"></div>	
</div>
	
</div>
<div class="second-game center">
	<h2>It's Bernard!</h2>
	<h3>Click each circle:</h3>
	<div id="game"></div>
	<br class="clear">
	<h2 class="center">I got the skills!</h2>	
</div>
 
 <div id="new-user-form">
 		<h4 class="center"><span id='winTime'></span></h4>
 		<h4 class="center">Sign up to save your time!</h4>
	<%= form_for(@user) do |f|  %>
		
		<% if @user.errors.messages.any? %>
			<% @user.errors.full_messages.each do |msg| %>
				<p><%= msg  %></p>
			<% end %>
		<% end %>

		<%= f.hidden_field :time  %>

		<p><%= f.label :name  %><br>
		<%= f.text_field :name  %></p>
	
		<p><%= f.label :email  %><br>
		<%= f.text_field :email  %></p>
	
		<p><%= f.label :password  %><br>
		<%= f.password_field :password %></p>
		
		<p><%= f.label :password_confirmation, "Confirm"  %><br>
		<%= f.password_field :password_confirmation %></p>

		<br>
		<br>
		<p class="center"><%= f.submit "Save Your Time", class: "submit_btn" %></p>

	<% end %>
</div>

		