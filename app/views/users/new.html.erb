<div  class="clock center">
	
		<span id="plus_five">+5</span><span id="seconds">00</span>:<span id="minutes">00</span>
		<% if @users.count > 0 %>
			<span id="time_best">(<%= @users[0].time %> is the time to beat).</span>
		<% end %>	
</div>

